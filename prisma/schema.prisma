generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Orders {
  id String @id @default(uuid())
  username String 
  email String
  products Json
  total Int
  created_at  DateTime @default(now())
  @@map("orders")
}